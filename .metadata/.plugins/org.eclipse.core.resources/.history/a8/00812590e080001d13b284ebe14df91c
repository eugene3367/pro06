    <br><hr><br>
    <script>
        $(document).ready(function(){
            $("#btn1").click(function(){
				$.ajax({
					url:"/ajax/userList.do",
					//data:id,
					contentType:'application/json;charset=utf-8',
					method:"GET",
					dataType:"json"
				})				
				.done(function(res){
					console.log(res);
				})
				.fail(function(xhr, status, errorThrown){
					$("#msg").text("회원 목록 실패"+xhr.status);
				});
            });
        });
    </script>
